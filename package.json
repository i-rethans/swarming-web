{
  "name": "9to5-nuxt-starter",
  "private": true,
  "engines": {
    "node": ">=20.5.1"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint . --fix",
    "*.js": "eslint . --fix",
    "*.vue": "eslint . --fix",
    "*.json": "prettier --write",
    "*.scss": "prettier --write",
    "*.md": "prettier --write",
    "*.yaml": "prettier --write"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "3.0.0-next.3",
    "@nuxt/content": "^2.8.2",
    "@nuxt/devtools": "^0.8.4",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxtjs/google-fonts": "^3.0.2",
    "@nuxtjs/i18n": "8.0.0-rc.5",
    "@types/node": "^20.6.2",
    "@types/phoenix": "^1.6.1",
    "@types/uuid": "^9.0.4",
    "@vueuse/core": "^10.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "leader-line-vue": "^2.1.1",
    "lint-staged": "^14.0.1",
    "nuxt": "^3.7.3",
    "prettier": "3.0.2",
    "qrcode.vue": "^3.4.1",
    "sass": "^1.67.0",
    "simple-git-hooks": "^2.9.0"
  },
  "dependencies": {
    "@nuxtjs/plausible": "^0.2.3",
    "phoenix": "^1.7.7",
    "uuid": "^9.0.1"
  }
}
